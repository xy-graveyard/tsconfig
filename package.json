{
  "$schema": "http://json.schemastore.org/package.json",
  "author": {
    "email": "support@xylabs.com",
    "name": "XY Labs Development Team",
    "url": "https://xylabs.com"
  },
  "bugs": {
    "email": "support@xylabs.com",
    "url": "https://github.com/xylabs/tsconfig/issues"
  },
  "description": "Typescript Base Config used throughout XY Labs TypeScript/JavaScript libraries and react projects",
  "devDependencies": {
    "@xylabs/eslint-config": "^2.1.6",
    "eslint": "^8",
    "prettier": "^2",
    "typescript": "^4"
  },
  "homepage": "https://github.com/xylabs/tsconfig.git",
  "keywords": [
    "xylabs",
    "utility",
    "config",
    "typescript",
    "tsconfig"
  ],
  "license": "LGPL-3.0",
  "main": "./index.json",
  "name": "@xylabs/tsconfig",
  "peerDependencies": {
    "eslint": "^8",
    "prettier": "^2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xylabs/tsconfig.git"
  },
  "scripts": {
    "deploy": "yarn version --patch && yarn publish ./",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install"
  },
  "sideEffects": false,
  "version": "1.0.5"
}
